{
  "name": "@peachjar/action-set-deployment-status",
  "version": "0.0.1",
  "private": true,
  "description": "Update the deployment status on a Github deployment",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "pack": "ncc build",
    "test": "./node_modules/.bin/jest --config jest.config.js --forceExit",
    "coverage": "./node_modules/.bin/jest --config jest.config.js --forceExit --coverage --verbose",
    "all": "npm run build && npm run lint && npm run pack && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "rclayton-the-terrible",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.6.4"
  }
}
